<Window x:Class="GooCooAdmin.LoginDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="用户登陆" Height="294.416" Width="458.642" ResizeMode="NoResize">
    <Canvas Margin="0,0,4,-19" Background="White">
        <Button x:Name="bn_login" Content="登录" Canvas.Left="322" Canvas.Top="216" Width="75" IsDefault="True"/>
        <Button x:Name="bn_cancel" Content="取消" Canvas.Left="219" Canvas.Top="216" Width="75"/>
        <TextBox x:Name="tb_id" Height="23" Canvas.Left="88" TextWrapping="Wrap" Canvas.Top="124" Width="309"/>
        <PasswordBox x:Name="pb_pw" Height="23" Canvas.Left="88" Canvas.Top="167" Width="309"/>
        <Label Content="密码" Canvas.Left="40" Canvas.Top="167"/>
        <Label Content="账号" Canvas.Left="40" Canvas.Top="124"/>
        <Image Height="109" Canvas.Left="107" Canvas.Top="10" Stretch="UniformToFill" Width="224" Source="Google Camp_LOGO.jpg"></Image>
    </Canvas>
</Window>
